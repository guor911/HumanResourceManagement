<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>HumanResourceManagement</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>HumanListServlet</servlet-name>
    <servlet-class>com.hrm.servlet.HumanListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HumanListServlet</servlet-name>
    <url-pattern>/list</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>HumanDeleteServlet</servlet-name>
    <servlet-class>com.hrm.servlet.HumanDeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HumanDeleteServlet</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>HumanUpdateServlet</servlet-name>
    <servlet-class>com.hrm.servlet.HumanUpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HumanUpdateServlet</servlet-name>
    <url-pattern>/update</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>HumanEditServlet</servlet-name>
    <servlet-class>com.hrm.servlet.HumanEditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HumanEditServlet</servlet-name>
    <url-pattern>/edit</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>HumanShowInfoServlet</servlet-name>
    <servlet-class>com.hrm.servlet.HumanShowInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HumanShowInfoServlet</servlet-name>
    <url-pattern>/info</url-pattern>
  </servlet-mapping>
</web-app>